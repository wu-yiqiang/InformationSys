{
  "name": "vite-plugin-mock",
  "version": "2.3.0",
  "description": "A mock plugin for vite",
  "main": "dist/index.js",
  "files": [
    "dist",
    "es"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf es",
    "dev": " tsup src/index.ts --dts --format cjs,esm --watch",
    "dev:client": "tsc -w --p tsconfig.es.json",
    "build": "npm run clean  && tsup src/index.ts --dts --format cjs,esm && tsc -p tsconfig.es.json",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "pinst --disable && npm run build",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "install:husky": "is-ci || husky install",
    "_postinstall": "npm run install:husky",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "vite",
    "mock",
    "hmr"
  ],
  "author": "Vben",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/anncwb/vite-plugin-mock"
  },
  "bugs": {
    "url": "https://github.com/anncwb/vite-plugin-mock/issues"
  },
  "homepage": "https://github.com/anncwb/vite-plugin-mock/tree/master/#readme",
  "dependencies": {
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@types/mockjs": "^1.0.3",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "connect": "^3.7.0",
    "debug": "^4.3.2",
    "esbuild": "^0.9.3",
    "fast-glob": "^3.2.5",
    "path-to-regexp": "^6.2.0",
    "rollup": "^2.41.5",
    "rollup-plugin-esbuild": "^3.0.2"
  },
  "peerDependencies": {
    "mockjs": ">=1.1.0",
    "vite": ">=2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.0",
    "@commitlint/config-conventional": "^12.1.0",
    "@types/connect": "^3.4.34",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "tsup": "^4.8.17",
    "typescript": "^4.2.3",
    "vite": "^2.1.2"
  }
}
